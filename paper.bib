@article{gordon2004surface,
  title={Surface subgroups of {Coxeter} and {Artin} groups},
  journal={Journal of Pure and Applied Algebra},
  volume={189},
  number={1},
  pages={135 - 148},
  year={2004},
  issn={0022-4049},
  doi={10.1016/j.jpaa.2003.10.011},
  url={https://doi.org/10.1016/j.jpaa.2003.10.011},
  author={Cameron Gordon and Darren Long and Alan Reid},
  abstract={We prove that any Coxeter group that is not virtually free contains a surface group. In particular if the Coxeter group is word hyperbolic and not virtually free this establishes the existence of a hyperbolic surface group, and answers in the affirmative a question of Gromov in this setting. We also discuss when Artin groups contain hyperbolic surface groups.}
}

@article{kim2014hyperbolic,
  author = {Kim, Sang{-}hyun and Oum, Sang{-}il},
  title = {Hyperbolic surface subgroups of one-ended doubles of free groups},
  journal = {Journal of Topology},
  volume = {7},
  number = {4},
  pages = {927-947},
  doi = {10.1112/jtopol/jtu004},
  url = {https://doi.org/10.1112/jtopol/jtu004},
  eprint = {https://londmathsoc.onlinelibrary.wiley.com/doi/pdf/10.1112/jtopol/jtu004},
  abstract = {Gromov asked whether every one-ended word-hyperbolic group contains a hyperbolic surface group. We prove that every one-ended double of a free group has a hyperbolic surface subgroup if (1) the free group has rank 2 or (2) every generator is used the same number of times in a minimal automorphic image of the amalgamating words. To prove this, we formulate a stronger statement on Whitehead graphs and prove its specialization by combinatorial induction for (1) and the characterization of perfect matching polytopes by Edmonds for (2).},
  year = {2014}
}

