@article{futer2012surface,
    author = {Futer, David and Thomas, Anne},
    title = {Surface Quotients of Hyperbolic Buildings},
    journal = {International Mathematics Research Notices},
    volume = {2012},
    number = {2},
    pages = {437-477},
    year = {2011},
    opt-month = {03},
    abstract = "{Let Ip,v be Bourdon’s building, the unique simply connected 2-complex such that all 2-cells are regular right-angled hyperbolic p-gons and the link at each vertex is the complete bipartite graph Kv,v. We investigate and mostly determine the set of triples (p,v,g) for which there exists a uniform lattice Γ=Γp,v,g in Aut(Ip,v) such that Γ\\Ip,v is a compact orientable surface of genus g. Surprisingly, for some p and g the existence of Γp,v,g depends upon the value of v. The remaining cases lead to open questions in tessellations of surfaces and in number theory. Our construction of Γp,v,g as the fundamental group of a simple complex of groups, together with a theorem of Haglund, implies that for p≥6 every uniform lattice in Aut(Ip,v) contains a surface subgroup. We use elementary group theory, combinatorics, algebraic topology, and number theory.}",
    issn = {1073-7928},
    doi = {10.1093/imrn/rnr028},
    url = {https://doi.org/10.1093/imrn/rnr028},
    eprint = {https://academic.oup.com/imrn/article-pdf/2012/2/437/2284330/rnr028.pdf},
}

@article{dahmani2011isomorphism,
author={Dahmani, Fran{\c{c}}ois
and Guirardel, Vincent},
title={The Isomorphism Problem for All Hyperbolic Groups},
journal={Geometric and Functional Analysis},
year={2011},
volume={21},
number={2},
pages={223-300},
abstract={We give a solution to Dehn's isomorphism problem for the class of all hyperbolic groups, possibly with torsion. We also prove a relative version for groups with peripheral structures. As a corollary, we give a uniform solution to Whitehead's problem asking whether two tuples of elements of a hyperbolic group G are in the same orbit under the action of Aut(G). We also get an algorithm computing a generating set of the group of automorphisms of a hyperbolic group preserving a peripheral structure.},
issn={1420-8970},
doi={10.1007/s00039-011-0120-0},
url={https://doi.org/10.1007/s00039-011-0120-0}
}

@article{dahmani2010foliations,
    author = {Dahmani, Fran{\c{c}}ois and Guirardel, Vincent},
    title = {Foliations for solving equations in groups: free, virtually free, and hyperbolic groups},
    journal = {Journal of Topology},
    volume = {3},
    number = {2},
    pages = {343-404},
    year = {2010},
    opt-month = {06},
    abstract = "{We give an algorithm for solving equations and inequations with rational constraints in virtually free groups. Our algorithm is based on Rips’ classification of measured band complexes. Using canonical representatives, we deduce an algorithm for solving equations and inequations in all hyperbolic groups (possibly with torsion). Additionally, we can deal with quasi-isometrically embeddable rational constraints.}",
    issn = {1753-8416},
    doi = {10.1112/jtopol/jtq010},
    url = {https://doi.org/10.1112/jtopol/jtq010},
    eprint = {https://academic.oup.com/jtopol/article-pdf/3/2/343/6930762/jtq010.pdf},
}

